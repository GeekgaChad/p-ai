# Postgres (RDS)
DB_URL=postgresql+psycopg2://postgres:REPLACE_WITH_PASSWORD@REPLACE_ENDPOINT:5432/p_ai?sslmode=require

# S3 + AWS
S3_BUCKET=your-bucket-name
AWS_REGION=ca-central-1
AWS_PROFILE=p-ai-dev

# Bedrock (set to your Bedrock region, e.g. us-west-2)
BEDROCK_REGION=us-west-2
BEDROCK_EMBED_MODEL=amazon.titan-embed-text-v2:0
BEDROCK_CHAT_MODEL=anthropic.claude-3-5-sonnet-20240620-v1:0

# Dev toggles
USE_PYMUPDF=0
DRY_INGEST=0
